class Lava {
    let root = script.Parent

    let touched = (part: BasePart) {
        let parent = part.Parent

        if parent {
            let humanoid = parent:FindFirstChild("Humanoid")

            if humanoid {
                humanoid:TakeDamage(100)
            }
        }
    }
}

let lava = Lava.new()

lava.root.Touched:Connect((part: BasePart) {
    lava.touched(lava, part)
})
